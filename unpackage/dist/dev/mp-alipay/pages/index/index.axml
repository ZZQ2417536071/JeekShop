<view class="home"><swiper indicator-dots autoplay="{{true}}" interval="{{2000}}" circular><block a:for="{{swipers}}" a:for-item="item" a:for-index="__i0__" a:key="id"><swiper-item><image src="{{item.img}}"></image></swiper-item></block></swiper><uni-grid vue-id="1" column="{{4}}" onVueInit="__l"><block a:for="{{navs}}" a:for-item="item" a:for-index="__i1__" a:key="title"><uni-grid-item vue-id="{{('2-'+__i1__)+','+('1')}}" onVueInit="__l"><view data-event-opts="{{[['tap',[['goSuperMarket',['$0'],[[['navs','title',item.title]]]]]]]}}" class="{{(item.icons)}}" onTap="__e"></view><text>{{item.title}}</text></uni-grid-item></block></uni-grid><goodlist vue-id="3" goods="{{goods}}" onVueInit="__l"></goodlist><block a:if="{{!flag}}"><uni-load-more vue-id="4" status="loading" onVueInit="__l"></uni-load-more></block><block a:else><uni-load-more vue-id="5" status="noMore" onVueInit="__l"></uni-load-more></block></view>